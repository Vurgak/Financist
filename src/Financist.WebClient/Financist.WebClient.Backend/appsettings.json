{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "webapi": {
        "ClusterId": "webapi",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "webapi": {
        "Destinations": {
          "destination1": {
            "Address": "https://webapi"
          }
        }
      }
    },
    "Exclusions": {
      "webapi": [
        "/authenticate",
        "/token"
      ]
    }
  },
  "Sessions": {
    "Expiration": 28800
  }
}
